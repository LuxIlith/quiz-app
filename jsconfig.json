// jsconfig.json
{
    "compilerOptions": {
        "baseUrl": "./",
        "paths": {
            "@components/*": [
                "src/components/*"
            ],
            "@pages/*": [
                "src/pages/*"
            ],
            "@styles/*": [
                "src/styles/*"
            ],
            "@icons/*": [
                "src/icons/*"
            ],
            "@data/*": [
                "src/data/*"
            ],
        }
    }
}